\ProvidesPackage{material-symbols}[2024/08/14 v0.0.1 Material Symbols]

\RequirePackage{fontspec}
\RequirePackage{xstring}
\RequirePackage{pgfkeys}

\newfontfamily{\MaterialSymbolsOutlined}[
    Path=fonts/,
    Extension=.ttf,
]{MaterialSymbolsOutlined-Regular}

\newfontfamily{\MaterialSymbolsRounded}[
    Path=fonts/,
    Extension=.ttf,
]{MaterialSymbolsRounded-Regular}

\newfontfamily{\MaterialSymbolsSharp}[
    Path=fonts/,
    Extension=.ttf,
]{MaterialSymbolsSharp-Regular}

\input{name-casting}

\newcommand{\mSymbolCode}[2][regular]{%
    {%
        \IfEqCase{#1}{%
            {outlined}{\MaterialSymbolsOutlined\symbol{"#2}}%
            {rounded}{\MaterialSymbolsRounded\symbol{"#2}}%
            {sharp}{\MaterialSymbolsSharp\symbol{"#2}}%
        }[\MaterialSymbolsOutlined\symbol{"#2}]%
    }%
}

\newcommand{\mSymbol}[2][regular]{%
  \pgfkeysgetvalue{/mSymbols/#2}\tempSymbolCode%
  \mSymbolCode[#1]{\tempSymbolCode}%
}

\endinput